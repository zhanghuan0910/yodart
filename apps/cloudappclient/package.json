{
  "name": "@yoda/cloudappclient",
  "version": "1.0.0",
  "description": "to parse cloud nlp",
  "main": "app.js",
  "manifest": {
    "daemon": true,
    "skills": [
      "@cloud"
    ],
    "permission": [
      "ACCESS_TTS",
      "ACCESS_MULTIMEDIA",
      "ACCESS_VOICE_COMMAND",
      "INTERRUPT"
    ],
    "hosts": [
      [ "cloudappclient", { "skillId": "@cloud" } ]
    ],
    "notifications": [
      "on-start-shake",
      "on-stop-shake",
      "on-quite-back",
      "on-quite-front"
    ]
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "iotjs ./tests/index.js",
    "travis": "node ./tests/index.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "extapp",
    "vui"
  ],
  "author": "sudo <xiaofei.lan@rokid.com>",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": ""
}
